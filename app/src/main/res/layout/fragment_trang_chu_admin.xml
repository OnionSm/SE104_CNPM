<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fragment.TrangChuAdminFragment">

    <ImageView
        android:id="@+id/trang_chu_logo_uit"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@id/trang_chu_guideline_1_4"
        app:layout_constraintEnd_toEndOf="@id/trang_chu_guideline_1"
        app:layout_constraintStart_toStartOf="@id/trang_chu_guideline_1_2"
        app:layout_constraintTop_toTopOf="@id/trang_chu_guideline_1_3"
        app:srcCompat="@drawable/banner_uit_2" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.65" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_1_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_1_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.02" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_1_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.085" />


    <ImageButton
        android:id="@+id/trang_chu_thong_bao_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@id/trang_chu_guideline_2_3"
        app:layout_constraintEnd_toEndOf="@id/trang_chu_guideline_2_2"
        app:layout_constraintStart_toStartOf="@id/trang_chu_guideline_2_1"
        app:layout_constraintTop_toTopOf="@id/trang_chu_guideline_1_3"
        app:srcCompat="@drawable/thong_bao_icon" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_2_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.905" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_2_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_2_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.048" />


    <de.hdodenhof.circleimageview.CircleImageView
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/trang_chu_user_image"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/male_default_avatar"

        app:civ_border_color="#FF000000"
        app:layout_constraintBottom_toBottomOf="@id/trang_chu_guideline_3_3"
        app:layout_constraintEnd_toEndOf="@id/trang_chu_guideline_1"
        app:layout_constraintStart_toStartOf="@id/trang_chu_guideline_3_2"
        app:layout_constraintTop_toTopOf="@id/trang_chu_guideline_3_1"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_3_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
