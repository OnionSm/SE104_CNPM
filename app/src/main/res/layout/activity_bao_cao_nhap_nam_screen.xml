<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BaoCaoNhapNamScreen">

    <ImageButton
        android:id="@+id/tao_de_thi_icon_back"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/them_cau_hoi_image_view_1"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@id/tao_de_guideline_1_2"
        app:layout_constraintEnd_toEndOf="@id/tao_de_guideline_1_4"
        app:layout_constraintStart_toStartOf="@id/tao_de_guideline_1_3"
        app:layout_constraintTop_toTopOf="@id/tao_de_guideline_1_1"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_1_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_1_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.082" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_1_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.06" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_1_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.09" />

    <ImageButton
        android:id="@+id/xem_bao_cao_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/xem_bao_cao_button"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@id/tao_de_guideline_1_2"
        app:layout_constraintEnd_toEndOf="@id/tao_de_guideline_2_2"
        app:layout_constraintStart_toStartOf="@id/tao_de_guideline_2_1"
        app:layout_constraintTop_toTopOf="@id/tao_de_guideline_1_1"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_2_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.755" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_2_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.955"/>

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/bao_cao_nam_text"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@id/tao_de_guideline_3_4"
        app:layout_constraintEnd_toEndOf="@id/tao_de_guideline_3_2"
        app:layout_constraintStart_toStartOf="@id/tao_de_guideline_3_1"
        app:layout_constraintTop_toTopOf="@id/tao_de_guideline_3_3"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_3_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.14" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_3_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.405"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_3_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.053" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_3_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.077" />


    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/nhap_nam_xem_bc_text"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@id/tao_de_guideline_4_4"
        app:layout_constraintEnd_toEndOf="@id/tao_de_guideline_4_2"
        app:layout_constraintStart_toStartOf="@id/tao_de_guideline_4_1"
        app:layout_constraintTop_toTopOf="@id/tao_de_guideline_4_3"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_4_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.175" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_4_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.82"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_4_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.156" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_4_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.182" />


    <ImageView
        android:id="@+id/chon_nam_background"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/rectangle_background_1"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@id/tao_de_guideline_5_4"
        app:layout_constraintEnd_toEndOf="@id/tao_de_guideline_5_2"
        app:layout_constraintStart_toStartOf="@id/tao_de_guideline_5_1"
        app:layout_constraintTop_toTopOf="@id/tao_de_guideline_5_3"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_5_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.055" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_5_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.945"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_5_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.377" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_5_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.585" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@id/tao_de_guideline_6_3"
        app:layout_constraintBottom_toBottomOf="@id/tao_de_guideline_6_4"
        app:layout_constraintStart_toStartOf="@id/chon_nam_background"
        app:layout_constraintEnd_toEndOf="@id/chon_nam_background"
        android:orientation="horizontal">

        <EditText
            android:id="@+id/otp_input_1"
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:imeOptions="actionNext"
            android:importantForAutofill="no"
            android:inputType="number"
            android:maxLength="4"
            android:gravity="center"
            android:textColor="#0000FD"
            android:textSize="24sp"
            android:text=""
            android:textStyle="bold"
            tools:ignore="LabelFor" />
        <TextView
            android:layout_width="30dp"
            android:layout_height="match_parent"
            android:layout_marginStart="4dp"
            android:layout_gravity="center"
            android:imeOptions="actionNext"
            android:importantForAutofill="no"
            android:inputType="number"
            android:gravity="center"
            android:maxLength="1"
            android:text="-"
            android:textColor="#2F6BFE"
            android:textSize="30sp"
            android:textStyle="bold"
            tools:ignore="LabelFor" />

        <EditText
            android:id="@+id/otp_input_2"
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:layout_marginStart="4dp"
            android:layout_gravity="center"
            android:imeOptions="actionNext"
            android:importantForAutofill="no"
            android:inputType="number"
            android:gravity="center"
            android:maxLength="4"
            android:textColor="#0000FD"
            android:textSize="24sp"
            android:textStyle="bold"
            android:text=""
            tools:ignore="LabelFor" />
    </LinearLayout>



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_6_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.235" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_6_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.455"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_6_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.445" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tao_de_guideline_6_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.51" />

</androidx.constraintlayout.widget.ConstraintLayout>