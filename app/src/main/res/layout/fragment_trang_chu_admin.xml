    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_10_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.79" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_10_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.857" />

    <ImageButton
        android:id="@+id/trang_chu_tham_so_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00FFFFFF"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@id/trang_chu_guideline_11_2"
        app:layout_constraintEnd_toEndOf="@id/trang_chu_guideline_6_4"
        app:layout_constraintStart_toStartOf="@id/trang_chu_guideline_6_3"
        app:layout_constraintTop_toTopOf="@id/trang_chu_guideline_11_1"
