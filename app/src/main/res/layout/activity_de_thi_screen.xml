<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/de_thi_screen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DeThiScreen">


    <ImageButton
        android:id="@+id/de_thi_icon_back"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@id/de_thi_guideline_1_4"
        app:layout_constraintEnd_toEndOf="@id/de_thi_guideline_1_2"
        app:layout_constraintStart_toStartOf="@id/de_thi_guideline_1_1"
        app:layout_constraintTop_toTopOf="@id/de_thi_guideline_1_3"
        app:srcCompat="@drawable/them_cau_hoi_image_view_1"
        tools:ignore="TouchTargetSizeCheck, SpeakableTextPresentCheck" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/de_thi_guideline_1_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.07"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/de_thi_guideline_1_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/de_thi_guideline_1_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.08"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/de_thi_guideline_1_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.105"/>


    <ImageView
        android:id="@+id/tao_de_thi_text"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@id/de_thi_guideline_1_4"
        app:layout_constraintEnd_toEndOf="@id/de_thi_guideline_2_2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/de_thi_guideline_2_1"
        app:layout_constraintTop_toTopOf="@id/de_thi_guideline_1_3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/de_thi_text" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/de_thi_guideline_2_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.13"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/de_thi_guideline_2_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.3"/>


    <ImageButton
        android:id="@+id/de_thi_tao_de_thi_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@id/de_thi_guideline_3_2"
        app:layout_constraintDimensionRatio="1.8:1"

        app:layout_constraintEnd_toEndOf="@id/de_thi_guideline_3_3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/de_thi_guideline_3_1"
        app:srcCompat="@drawable/tao_de_thi_image_button"
        tools:ignore="SpeakableTextPresentCheck"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/de_thi_guideline_3_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.31"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/de_thi_guideline_3_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.51"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/de_thi_guideline_3_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.76"/>


    <ImageButton
        android:id="@+id/tao_de_thi_danh_sach_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@id/de_thi_guideline_4_3"
        app:layout_constraintDimensionRatio="2:1"
        app:layout_constraintEnd_toEndOf="@id/de_thi_guideline_4_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/de_thi_guideline_4_1"
        app:srcCompat="@drawable/danh_sach_de_thi_image_button"
        tools:ignore="SpeakableTextPresentCheck"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/de_thi_guideline_4_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.48"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/de_thi_guideline_4_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.68"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/de_thi_guideline_4_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.65"/>

</androidx.constraintlayout.widget.ConstraintLayout>

