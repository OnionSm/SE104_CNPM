    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_6_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.075" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_6_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.925" />


    <ImageButton
        android:id="@+id/trang_chu_de_thi_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00FFFFFF"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@id/trang_chu_guideline_7_2"
        app:layout_constraintEnd_toEndOf="@id/trang_chu_guideline_6_4"
        app:layout_constraintStart_toStartOf="@id/trang_chu_guideline_6_3"
        app:layout_constraintTop_toTopOf="@id/trang_chu_guideline_7_1"
        app:srcCompat="@drawable/trang_chu_de_thi_button" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_7_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.535" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_7_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.602" />


    <ImageButton
        android:id="@+id/trang_chu_tra_cuu_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00FFFFFF"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@id/trang_chu_guideline_8_2"
        app:layout_constraintEnd_toEndOf="@id/trang_chu_guideline_6_4"
