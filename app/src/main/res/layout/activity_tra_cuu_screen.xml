<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tra_cuu_screen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TraCuuScreen">

    <ImageButton
        android:id="@+id/tra_cuu_back_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/them_cau_hoi_image_view_1"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@id/tra_cuu_guideline_1_2"
        app:layout_constraintEnd_toEndOf="@id/tra_cuu_guideline_1_4"
        app:layout_constraintStart_toStartOf="@id/tra_cuu_guideline_1_3"
        app:layout_constraintTop_toTopOf="@id/tra_cuu_guideline_1_1"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_1_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_1_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.082" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_1_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.06" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_1_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.09" />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/tra_cuu_text"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@id/tra_cuu_guideline_2_2"
        app:layout_constraintEnd_toEndOf="@id/tra_cuu_guideline_2_4"
        app:layout_constraintStart_toStartOf="@id/tra_cuu_guideline_2_3"
        app:layout_constraintTop_toTopOf="@id/tra_cuu_guideline_2_1"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_2_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.053" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_2_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.078" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_2_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.14" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_2_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.29" />


    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/tra_cuu_input_background"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@id/tra_cuu_guideline_3_2"
        app:layout_constraintEnd_toEndOf="@id/tra_cuu_guideline_3_4"
        app:layout_constraintStart_toStartOf="@id/tra_cuu_guideline_3_3"
        app:layout_constraintTop_toTopOf="@id/tra_cuu_guideline_3_1"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_3_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.172" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_3_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.265" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_3_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.01" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_3_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.99"/>

    <SearchView
        android:id="@+id/tra_cuu_search_view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        android:queryHint="Nhập mã số đề thi"
        android:iconifiedByDefault="false"
        android:fontFamily="@font/roboto_regular"
        app:srcCompat="@drawable/tra_cuu_input_background"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@id/tra_cuu_guideline_4_2"
        app:layout_constraintEnd_toEndOf="@id/tra_cuu_guideline_4_4"
        app:layout_constraintStart_toStartOf="@id/tra_cuu_guideline_4_3"
        app:layout_constraintTop_toTopOf="@id/tra_cuu_guideline_4_1">
        <requestFocus/>
    </SearchView>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_4_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.2" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_4_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.225" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_4_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.09" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_4_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.83" />

    <ImageButton
        android:id="@+id/tra_cuu_mo_rong_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/tra_cuu_expand_button"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@id/tra_cuu_guideline_4_2"
        app:layout_constraintEnd_toEndOf="@id/tra_cuu_guideline_5_3"
        app:layout_constraintStart_toStartOf="@id/tra_cuu_guideline_5_2"
        app:layout_constraintTop_toTopOf="@id/tra_cuu_guideline_5_1"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_5_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.195" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_5_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.865" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_5_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.935" />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/tra_cuu_gan_day_nhat_text"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@id/tra_cuu_guideline_6_2"
        app:layout_constraintEnd_toEndOf="@id/tra_cuu_guideline_6_4"
        app:layout_constraintStart_toStartOf="@id/tra_cuu_guideline_6_3"
        app:layout_constraintTop_toTopOf="@id/tra_cuu_guideline_6_1"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_6_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.315" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_6_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.335" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_6_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.048" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_6_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.195" />

    <ImageView
        android:id="@+id/tra_cuu_xem_tat_ca"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/tra_cuu_xem_tat_ca_text"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@id/tra_cuu_guideline_6_2"
        app:layout_constraintEnd_toEndOf="@id/tra_cuu_guideline_7_2"
        app:layout_constraintStart_toStartOf="@id/tra_cuu_guideline_7_1"
        app:layout_constraintTop_toTopOf="@id/tra_cuu_guideline_6_1"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_7_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.817" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_7_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/ds_de_thi_rcv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/tra_cuu_guideline_7_2"
        app:layout_constraintStart_toStartOf="@id/tra_cuu_guideline_6_3"
        app:layout_constraintTop_toTopOf="@id/tra_cuu_guideline_8_1"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tra_cuu_guideline_8_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.345"/>



</androidx.constraintlayout.widget.ConstraintLayout>