<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profile"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/them_cau_hoi_shape_1">
        <ImageView
            android:layout_marginTop="-45dp"
            android:layout_width="200dp"
            android:layout_height="160dp"
            android:layout_marginLeft="20dp"
            android:src="@drawable/banner_uit_2"
            android:layout_gravity="top|left"/>
        <ImageView
            android:id="@+id/profile_image_view_1"
            android:layout_marginTop="20dp"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginLeft="315dp"
            android:src="@drawable/bell"
            android:layout_gravity="top|left"/>
        <ImageView
            android:layout_width="120dp"
            android:layout_height="130dp"
            android:src="@drawable/user_image"
            android:layout_marginTop="100dp"
            android:layout_gravity="center_horizontal"/>
        <FrameLayout
            android:layout_width="300dp"
            android:layout_height="45dp"
            android:layout_marginTop="240dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="top|left"
                android:fontFamily="@font/roboto_regular"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Tên"
                android:textColor="#0000FD"
                android:textSize="15dp"
                android:textStyle="bold"
                tools:ignore="TextSizeCheck" />
        <TextView
            android:id="@+id/profile_text_view_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left|top"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/roboto_regular"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Ngô Đức Thành"
            android:textColor="#6E7591"
            android:textSize="16dp"
            android:textStyle="bold"
            tools:ignore="TextSizeCheck" />
            <ImageView
                android:layout_width="310dp"
                android:layout_height="2dp"
                android:layout_marginTop="40dp"
                android:src="@drawable/profile_image_view_line"/>
        </FrameLayout>
        <FrameLayout
            android:layout_width="300dp"
            android:layout_height="45dp"
            android:layout_marginTop="310dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="top|left"
                android:fontFamily="@font/roboto_regular"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Ngày sinh"
                android:textColor="#0000FD"
                android:textSize="15dp"
                android:textStyle="bold"
                tools:ignore="TextSizeCheck" />
            <TextView
                android:id="@+id/profile_text_view_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|top"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/roboto_regular"
                android:maxLines="1"
                android:singleLine="true"
                android:text="05/11/1993"
                android:textColor="#6E7591"
                android:textSize="16dp"
                android:textStyle="bold"
                tools:ignore="TextSizeCheck" />
            <ImageView
                android:layout_width="310dp"
                android:layout_height="2dp"
                android:layout_marginTop="40dp"
                android:src="@drawable/profile_image_view_line"/>
        </FrameLayout>
        <FrameLayout
            android:layout_width="300dp"
            android:layout_height="45dp"
            android:layout_marginTop="380dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="top|left"
                android:fontFamily="@font/roboto_regular"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Giới tính"
                android:textColor="#0000FD"
                android:textSize="15dp"
                android:textStyle="bold"
                tools:ignore="TextSizeCheck" />
            <TextView
                android:id="@+id/profile_text_view_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|top"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/roboto_regular"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Nam"
                android:textColor="#6E7591"
                android:textSize="16dp"
                android:textStyle="bold"
                tools:ignore="TextSizeCheck" />
            <ImageView
                android:layout_width="310dp"
                android:layout_height="1dp"
                android:layout_marginTop="40dp"
                android:src="@drawable/profile_image_view_line"/>
        </FrameLayout>
        <FrameLayout
            android:layout_width="300dp"
            android:layout_height="45dp"
            android:layout_marginTop="450dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="top|left"
                android:fontFamily="@font/roboto_regular"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Email"
                android:textColor="#0000FD"
                android:textSize="15dp"
                android:textStyle="bold"
                tools:ignore="TextSizeCheck" />
            <TextView
                android:id="@+id/profile_text_view_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|top"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/roboto_regular"
                android:maxLines="1"
                android:singleLine="true"
                android:text="thanhnd@uit.edu.vn"
                android:textColor="#6E7591"
                android:textSize="16dp"
                android:textStyle="bold"
                tools:ignore="TextSizeCheck" />
            <ImageView
                android:layout_width="310dp"
                android:layout_height="1dp"
                android:layout_marginTop="41dp"
                android:src="@drawable/profile_image_view_line"/>
        </FrameLayout>
        <FrameLayout
            android:layout_width="300dp"
            android:layout_height="45dp"
            android:layout_marginTop="520dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="top|left"
                android:fontFamily="@font/roboto_regular"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Số điện thoại"
                android:textColor="#0000FD"
                android:textSize="15dp"
                android:textStyle="bold"
                tools:ignore="TextSizeCheck" />
            <TextView
                android:id="@+id/profile_text_view_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|top"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/roboto_regular"
                android:maxLines="1"
                android:singleLine="true"
                android:text="-"
                android:textColor="#6E7591"
                android:textSize="16dp"
                android:textStyle="bold"
                tools:ignore="TextSizeCheck" />
            <ImageView
                android:layout_width="310dp"
                android:layout_height="1dp"
                android:layout_marginTop="40dp"
                android:src="@drawable/profile_image_view_line"/>
        </FrameLayout>
        <FrameLayout
            android:layout_width="300dp"
            android:layout_height="45dp"
            android:layout_marginTop="590dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="top|left"
                android:fontFamily="@font/roboto_regular"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Địa chỉ"
                android:textColor="#0000FD"
                android:textSize="15dp"
                android:textStyle="bold"
                tools:ignore="TextSizeCheck" />
            <TextView
                android:id="@+id/profile_text_view_6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|top"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/roboto_regular"
                android:maxLines="1"
                android:singleLine="true"
                android:text="-"
                android:textColor="#6E7591"
                android:textSize="16dp"
                android:textStyle="bold"
                tools:ignore="TextSizeCheck" />
            <ImageView
                android:layout_width="310dp"
                android:layout_height="1dp"
                android:layout_marginTop="44dp"
                android:src="@drawable/profile_image_view_line"/>
        </FrameLayout>
        <FrameLayout
            android:layout_width="361dp"
            android:layout_height="50dp"
            android:layout_gravity="top|left"
            android:scaleType="centerInside"
            android:background="@drawable/bar"
            android:layout_marginTop="710dp">
            <ImageView
                android:id="@+id/profile_image_view_2"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="30dp"
                android:src="@drawable/profile_image_view"
                android:layout_gravity="top|left"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Thông báo"
                android:textColor="#0000FD"
                android:fontFamily="@font/roboto_regular"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="20dp"
                android:textSize="10sp"/>
        <ImageView
            android:id="@+id/profile_image_view_3"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginTop="10dp"
            android:layout_gravity="center_horizontal"
            android:scaleType="centerInside"
            android:src="@drawable/profile_home_image"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Trang chủ"
                android:textColor="#0000FD"
                android:fontFamily="@font/roboto_regular"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="160dp"
                android:textSize="10sp"/>
            <ImageView
                android:id="@+id/profile_image_view_4"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="300dp"
                android:scaleType="centerInside"
                android:src="@drawable/profile_user_image"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Hồ sơ"
                android:textColor="#0000FD"
                android:fontFamily="@font/roboto_regular"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="300dp"
                android:textSize="10sp"/>

        </FrameLayout>


    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>