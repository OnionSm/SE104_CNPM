        android:background="#00FFFFFF"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@id/trang_chu_guideline_9_2"
        app:layout_constraintEnd_toEndOf="@id/trang_chu_guideline_6_4"
        app:layout_constraintStart_toStartOf="@id/trang_chu_guideline_6_3"
        app:layout_constraintTop_toTopOf="@id/trang_chu_guideline_9_1"
        app:srcCompat="@drawable/trang_chu_cham_diem_button"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_9_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
