        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.356" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="Khoa Khoa học máy tính"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@id/trang_chu_guideline_5_1"
        app:layout_constraintDimensionRatio="10:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/trang_chu_guideline_4_2" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/trang_chu_guideline_5_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.41" />
    <ImageButton
        android:id="@+id/trang_chu_cau_hoi_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00FFFFFF"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@id/trang_chu_guideline_6_2"
        app:layout_constraintEnd_toEndOf="@id/trang_chu_guideline_6_4"
        app:layout_constraintStart_toStartOf="@id/trang_chu_guideline_6_3"
        app:layout_constraintTop_toTopOf="@id/trang_chu_guideline_6_1"
