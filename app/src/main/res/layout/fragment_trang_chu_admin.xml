    <ImageButton
        android:id="@+id/trang_chu_bao_cao_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00FFFFFF"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@id/trang_chu_guideline_10_2"
        app:layout_constraintEnd_toEndOf="@id/trang_chu_guideline_6_4"
